<configuration xml:space="preserve">
    <config name="MappingName" xml:space="preserve">

        
        <property source="sp-ackDate" target="AcknowledgmentDate" xml:space="preserve">
            <source xml:space="preserve">
                <format xml:space="preserve">dd/MM/yyyy</format>
            </source>
        </property>
        <property source="sp-ackInfo" target="AcknowledgmentInformations" />
        <property source="sp-ackLevel" target="AcknowledgmentLevel" defaultSource="Registered" />
        <property source="sp-ackRef" target="AcknowledgmentReference" />
        <property source="sp-alertDate" target="AlertDate" xml:space="preserve">
            <source xml:space="preserve">
                <format xml:space="preserve">dd-MM-yyyy</format>
            </source>
        </property>
        <property source="sp-comment" target="Commentary" />
        <property source="sp-confidentiality" target="Confidentiality" />
        <property source="sp-description" target="Description" />
        <property source="sp-dir" target="Direction" xml:space="preserve">
            <map xml:space="preserve">
                <add key="0" value="OutGoingMail" />
                <add key="1" value="InMail" />
                <add key="2" value="InternalMail" />
            </map>
        </property>
        <property source="sp-dueDate" target="DueDate" xml:space="preserve">
            <source xml:space="preserve">
                <format xml:space="preserve">dd-MM-yyyy</format>
            </source>
        </property>
        <property source="sp-ackAvailable" target="IsAcknowledgmentAvailable" xml:space="preserve">
            <map xml:space="preserve">
                <add key="+" value="true" />
                <add key="-" value="false" />
            </map>
        </property>
        <property source="sp-date" target="MailDate" xml:space="preserve">
            <source xml:space="preserve">
                <regex />
                <format xml:space="preserve">yyyy-MM-dd</format>
            </source>
        </property>
        <property source="sp-type" target="MailType" defaultSource="myKey" xml:space="preserve">
            <map xml:space="preserve">
                
                <add key="myKey" value="STANDARD" type="code" />
                <sort xml:space="preserve">
                    <id xml:space="preserve">TRI_COURRIERS_TYPE_SOMMAIRE</id>
                    <set xml:space="preserve">COURRIERS_TYPE</set>
                    <form xml:space="preserve">0</form>
                    <key xml:space="preserve">1</key>
                </sort>
            </map>
        </property>
        <property source="sp-priority" target="Priority" />
        <property source="sp-ref" target="Reference" />
        <property source="sp-reported" target="Reported" />
        <property source="sp-sendingDate" target="SendingDate" />
        <property source="sp-subject" target="Subject" defaultSource="" xml:space="preserve">
            <map xml:space="preserve">
                <add key="" value="ceci est un message de test" />
            </map>
        </property>
        <property source="sp-support" target="Support" defaultSource="myKey" xml:space="preserve">
            <source xml:space="preserve">
                <regex />
            </source>
            <map defaultItemType="code" xml:space="preserve">
                <add key="myKey" value="PAPIER" type="code" />
                
            </map>
        </property>
        <property source="sp-redactor" target="Redactor" xml:space="preserve">
            <map xml:space="preserve">
                <ldapAttribute xml:space="preserve">p-adminDescription</ldapAttribute>
                <useMainService xml:space="preserve">false</useMainService>
            </map>
        </property>
        <property source="sp-signatory" target="Signatory" xml:space="preserve">
            <map xml:space="preserve">
                
                <ldapAttribute xml:space="preserve">email</ldapAttribute>
                
                <useMainService xml:space="preserve">false</useMainService>
            </map>
        </property>

        <property source="sp-sender" target="ExternalSender" xml:space="preserve">
            <map xml:space="preserve">
                
                <type xml:space="preserve">email</type>
                <addressBook xml:space="preserve">DEFAULT_ADDRESS_BOOK</addressBook>
            </map>
        </property>

        <property source="sp-internalRecipients" target="InternalRecipients" xml:space="preserve">
            <map xml:space="preserve">
                
                <ldapAttribute xml:space="preserve">displayName</ldapAttribute>
                
                <useMainService xml:space="preserve">false</useMainService>
            </map>
        </property>

        <property source="sp-externalRecipients" target="ExternalRecipients" xml:space="preserve">
            <map xml:space="preserve">
                
                <type xml:space="preserve">name</type>
                <addressBook xml:space="preserve">DEFAULT_ADDRESS_BOOK</addressBook>
            </map>
        </property>

        
        <property source="sp-cfcontact" target="CP_CONTACT" xml:space="preserve">
            <map xml:space="preserve">
                
                <type xml:space="preserve">name</type>
                <addressBook xml:space="preserve">DEFAULT_ADDRESS_BOOK</addressBook>
            </map>
        </property>

        <property source="sp-cfflwcht" target="CP_ORGA" xml:space="preserve">
            <map xml:space="preserve">
                <ldapAttribute xml:space="preserve">sid</ldapAttribute>
                <useMainService xml:space="preserve">true</useMainService>
            </map>
        </property>
        <property source="sp-cflex" target="CP_THESAU" xml:space="preserve">
            <map xml:space="preserve">
                
                <idExtraPropertyName />
            </map>
        </property>
        <property source="sp-cfref" target="CP_REF" xml:space="preserve">
            <map xml:space="preserve">
                <add key="myKey" value="A_DISTRIBUER" />
                <sort xml:space="preserve">
                    <id xml:space="preserve">TRI_COURRIERS_SUPPORT_SOMMAIRE</id>
                    <set xml:space="preserve">COURRIERS_SUPPORT</set>
                    <form xml:space="preserve">0</form>
                    <key xml:space="preserve">1</key>
                </sort>
            </map>
        </property>
        <property source="sp-cfamount" target="CP_MONTANT" xml:space="preserve">
            <source xml:space="preserve">
                <currency xml:space="preserve">$</currency>
                <decimalSeparator xml:space="preserve">.</decimalSeparator>
                <groupSeparator xml:space="preserve">,</groupSeparator>
            </source>
        </property>
        <property source="sp-cfphone" target="CP_TEL" />
        <property source="sp-cfmail" target="CP_MAIL" xml:space="preserve">
        </property>
        <property source="sp-cfnumber" target="CP_NOMBRE" xml:space="preserve">
        </property>
        <property source="sp-cftexte" target="CP_TEST" xml:space="preserve">
        </property>
        <property source="sp-cfdate" target="CP_DATE" xml:space="preserve">
            <source xml:space="preserve">
                <format xml:space="preserve">dd/MM/yyyy</format>
            </source>
        </property>
        <property source="sp-cfenum" target="CP_ENUM" xml:space="preserve">
        </property>
    </config>

    <config name="Open311" xml:space="preserve">
        
        <property source="o311-sender" target="ExternalSender" xml:space="preserve">
            <map xml:space="preserve">
                <type xml:space="preserve">name</type>
                <addressBook xml:space="preserve">DEFAULT_ADDRESS_BOOK</addressBook>
            </map>
        </property>
        <property source="o311-title" target="Subject" />
        <property source="o311-description" target="Description" />
        <property source="o311-mailType" target="MailType" />
        <property source="o311-support" target="Support" />
        <property source="o311-createdOn" target="MailDate" xml:space="preserve">
            <source xml:space="preserve">
                <format xml:space="preserve">dd/MM/yyyy HH:mm:ss</format>
            </source>
        </property>

        
        <property source="o311-serviceRequestId" target="CP_O311GR_SERVICE_REQUEST_ID" />
        <property source="o311-serviceCode" target="CP_O311GR_CITIZEN_SERVICE" xml:space="preserve">
            <map defaultItemType="id" />
        </property>
        <property source="o311-serviceNotice" target="CP_O311GR_SERVICE_NOTICE" />
        <property source="o311-adressString" target="CP_O311GR_REQ_ADDR" />
        <property source="o311-locale" target="CP_O311GR_LOCALE" />
        <property source="o311-fromApiKey" target="CP_O311GR_FROM_APP_KEY" />
        <property source="o311-deviceId" target="CP_O311GR_DEVICE_ID" />
        <property source="o311-email" target="CP_O311GR_EMAIL_REQUESTER" />
        <property source="o311-firstName" target="CP_O311GR_FIRST_NAME_REQUESTER" />
        <property source="o311-lastName" target="CP_O311GR_LAST_NAME_REQUESTER" />
        <property source="o311-phone" target="CP_O311GR_PHONE_REQUESTER" />
        <property source="o311-latitude" target="CP_O311GR_REQ_LAT" />
        <property source="o311-longitude" target="CP_O311GR_REQ_LONG" />
        <property source="o311-createdOn" target="CP_O311GR_REQ_DATE" xml:space="preserve">
            <source xml:space="preserve">
                <format xml:space="preserve">dd/MM/yyyy HH:mm:ss</format>
            </source>
        </property>
        <property source="o311-expectedDateTime" target="DueDate" xml:space="preserve">
            <source xml:space="preserve">
                <format xml:space="preserve">yyyyMMdd</format>
            </source>
        </property>
        <property source="o311-expectedDateTime" target="AlertDate" xml:space="preserve">
            <source xml:space="preserve">
                <format xml:space="preserve">yyyyMMdd</format>
            </source>
        </property>
        <property source="o311-modifiedOn" target="CP_O311GR_LAST_UPDATE_SENT" />
        <property source="o311-agencyResponsible" target="CP_O311GR_AGENCY_RESPONSIBLE" />
        <property source="o311-detailedStatus" target="CP_O311GR_DETAILED_STATUS" />
        <property source="o311-accountId" target="CP_O311GR_REQ_ACCOUNT_ID" />
        <property source="o311-geoReportStatus" target="CP_O311GR_STATUS" />
        <property source="o311-geoReportStatusNote" target="CP_O311GR_STATUS_NOTE" />

        

    </config>
</configuration>