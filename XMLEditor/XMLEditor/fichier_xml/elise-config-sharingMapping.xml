    <property source="MailId" target="sp-id" defaultSource="">
      <source>
        <regex>
          <![CDATA[
          
					(?<value>.*)
				
        ]]>
        </regex>
      </source>
      <map>
        <add key="" value="ceci est un message de test" />
      </map>
      <target>
        <format>
          <![CDATA[
          
					foo - {0}
				
        ]]>
        </format>
      </target>
    </property>
