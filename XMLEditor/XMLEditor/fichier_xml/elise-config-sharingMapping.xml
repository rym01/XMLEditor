  <config name="SharingMappingName">
    <filters>
      <property source="Direction">
        <predicates>
          <equals>All</equals>
          <!--<lessThan></lessThan>
          <lessThanOrEquals></lessThanOrEquals>
          <greaterThan></greaterThan>
          <greaterThanOrEquals></greaterThanOrEquals>
          <contains></contains>
          <startsWith></startsWith>
